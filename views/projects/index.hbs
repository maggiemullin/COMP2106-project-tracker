<h1>{{title}}</h1>
<a href="/projects/add">Add a New Project</a>

<table class="table table-striped table-hover">
    <thead>
    <th>Name</th>
    <th>Due Date</th>
    <th>Course</th>
    <th>Status</th>
    {{#each projects}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.dueDate}}</td>
            <td>{{this.course}}</td>
            <td>{{this.status}}</td>
        </tr>
    {{/each}}
    </thead>
</table>
